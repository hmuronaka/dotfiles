nmap gp :pc<cr>
nmap gn :nc<cr>
nmap ni :ni<cr>
nmap np :pi<cr>
nmap ,w :w<cr>

nmap gf :xccmd jumpToDefinition<cr>

imap <C-j> <ESC>
noremap <C-j> <C-w>j
